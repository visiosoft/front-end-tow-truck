{
  "expo": {
    "name": "tow-truck",
    "slug": "tow-truck",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#967969"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#967969"
      },
      "googleServicesFile": "./google-services.json",
      "package": "com.zainali950.towtruck"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/ProximaNova-Regular.otf",
            "./assets/fonts/ProximaNova-Bold.otf",
            "./assets/fonts/ProximaNova-Extrabold.otf",
            "./assets/fonts/ProximaNova-Semibold.otf",
            "./assets/fonts/ProximaNova-Thin.otf"
          ]
        }
      ],
      "expo-asset",
      "expo-secure-store",
      "expo-font",
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "62c643c8-9752-4d22-bb23-9b30e626f791"
      }
    }
  }
}